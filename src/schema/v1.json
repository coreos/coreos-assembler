      ]
    },
   "coreos-assembler.image-input-checksum": {
     "$id":"#/properties/coreos-assembler.image-input-checksum",
     "type":"string",
     "title":"Image input checksum",
     "default":"",
     "examples": [
       "59b0904f91aafcf55a66075b731476f802c9d60f17b0c670fb5c43d26333b876"
