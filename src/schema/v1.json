      ],
      "pattern":"[A-Fa-f0-9]{64}$"
    },
   "coreos-assembler.image-genver": {
     "$id":"#/properties/coreos-assembler.image-genver",
     "type":"integer",
     "title":"COSA Image Version",
     "default": 0,
     "examples": [
